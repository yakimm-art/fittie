{
  "name": "fittie",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered fitness coaching system with real-time form analysis and voice feedback",
  "workspaces": [
    "frontend",
    "backend",
    "infra",
    "shared"
  ],
  "scripts": {
    "install:all": "npm install",
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces --if-present",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,yml,yaml}\"",
    "clean": "npm run clean --workspaces --if-present && rm -rf node_modules",
    "dev:backend": "npm run dev -w backend",
    "dev:frontend": "npm run dev -w frontend",
    "deploy:infra": "npm run deploy -w infra",
    "synth:infra": "npm run synth -w infra"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/fittie.git"
  },
  "keywords": [
    "fitness",
    "ai",
    "coaching",
    "computer-vision",
    "voice-synthesis"
  ],
  "author": "",
  "license": "MIT"
}
