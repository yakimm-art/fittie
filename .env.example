# ElevenLabs Configuration
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1

# Cognito Configuration
COGNITO_USER_POOL_ID=us-east-1_xxxxxxxxx
COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
COGNITO_USER_POOL_ARN=arn:aws:cognito-idp:us-east-1:xxxxxxxxxxxx:userpool/us-east-1_xxxxxxxxx

# DynamoDB Tables
DYNAMODB_STATE_TABLE=user-physical-state
DYNAMODB_STATE_TABLE_ARN=arn:aws:dynamodb:us-east-1:xxxxxxxxxxxx:table/user-physical-state
DYNAMODB_STATE_TABLE_STREAM_ARN=arn:aws:dynamodb:us-east-1:xxxxxxxxxxxx:table/user-physical-state/stream/xxxx
DYNAMODB_HISTORY_TABLE=workout-history
DYNAMODB_HISTORY_TABLE_ARN=arn:aws:dynamodb:us-east-1:xxxxxxxxxxxx:table/workout-history
DYNAMODB_EXERCISE_TABLE=exercise-knowledge-base
DYNAMODB_EXERCISE_TABLE_ARN=arn:aws:dynamodb:us-east-1:xxxxxxxxxxxx:table/exercise-knowledge-base

# S3 Buckets
S3_PWA_BUCKET=fittie-pwa-xxxxxxxxxxxx
S3_PWA_BUCKET_ARN=arn:aws:s3:::fittie-pwa-xxxxxxxxxxxx
S3_PWA_WEBSITE_URL=http://fittie-pwa-xxxxxxxxxxxx.s3-website-us-east-1.amazonaws.com
S3_MEDIA_BUCKET=fittie-media-xxxxxxxxxxxx
S3_MEDIA_BUCKET_ARN=arn:aws:s3:::fittie-media-xxxxxxxxxxxx

# CloudFront Distribution
CLOUDFRONT_DISTRIBUTION_ID=XXXXXXXXXXXXXX
CLOUDFRONT_DOMAIN_NAME=xxxxxxxxxxxx.cloudfront.net
CLOUDFRONT_URL=https://xxxxxxxxxxxx.cloudfront.net

# Optional - Dreamflow Configuration
# DREAMFLOW_API_KEY=your_dreamflow_api_key_here
